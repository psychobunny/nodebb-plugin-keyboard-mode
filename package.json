{
  "name": "nodebb-plugin-keyboard-mode",
  "version": "0.0.1",
  "description": "Keyboard Mode Plugin for NodeBB",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/psychobunny/nodebb-plugin-keyboard-mode"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "keyboard",
    "shortcuts",
    "console"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "psychobunny",
    "email": "psycho.bunny@hotmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psychobunny/nodebb-plugin-keyboard-mode/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.14.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "^7.1.2",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^2.4.0",
    "lint-staged": "^10.0.9"
  }
}
